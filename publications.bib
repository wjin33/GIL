@inproceedings{ghassemi_hydraulic_2025,
	title = {Hydraulic {Fracturing} in {Geothermal} {Reservoirs}: {The} {Utah} {Forge} {EGS} and {Newberry} {Superhot} {Projects}},
	shorttitle = {Hydraulic {Fracturing} in {Geothermal} {Reservoirs}},
	url = {https://dx.doi.org/10.2118/223576-MS},
	abstract = {Recently, multi-stage hydraulic fracturing is being implemented in hot geothermal reservoirs both in enhanced geothermal systems (EGS) and superhot reservoirs such as the Utah FORGE and Newberry, respectively. Hydraulic fracturing in these fields poses distinct challenges to design and forecasting the fracturing outcome. This is rooted in the geomechanics aspects of these reservoirs and the high temperature conditions along with specific well completions strategies. In this paper, we present recent experiences in these two fields and present data and analysis to help better understand the stimulation and the resulting SRV and fracture geometries. We examine two conceptual models for hydraulic stimulation of geothermal reservoir namely, hydraulic fracture propagation and shear stimulation. We use fully coupled, 3D thermo-poroelastic DD method which considers planar and non-planar fracture propagation and interactions with natural fractures. We show that asymmetric and exotic growth behavior is to be expected in the stress regimes encountered in these reservoirs and completions styles used. We also assess the reservoir growth during circulation experiments with reference to effective reservoir management and power production strategies.},
