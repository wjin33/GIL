Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {CO$_{\textrm{2}}$ storage, CO$_{\textrm{2}}$ storage projects, CO$_{\textrm{2}}$ utilization, cost-effectiveness, security assessment},
	pages = {600},
	file = {Full Text PDF:/Users/wencheng/Zotero/storage/USQ2CRVD/Cao et al. - 2020 - A Review of CO2 Storage in View of Safety and Cost.pdf:application/pdf},
}

@article{stricker_potential_2020,
