	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/nme.7298},
	keywords = {fracture, DG, FRC, IIPG, intrinsic/extrinsic cohesive law},
	pages = {5356--5374},
	file = {Full Text PDF:/Users/wencheng/Zotero/storage/Z5YZ83CH/Liu et al. - 2023 - A robust interface finite element formulation for .pdf:application/pdf;Snapshot:/Users/wencheng/Zotero/storage/IBZ7QW8W/nme.html:text/html},
}

@article{kafashi_review_2023,
