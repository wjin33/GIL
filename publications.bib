@article{list_learned_2022,
	title = {Learned {Turbulence} {Modelling} with {Differentiable} {Fluid} {Solvers}: {Physics}-based {Loss}-functions and {Optimisation} {Horizons}},
	volume = {949},
	issn = {0022-1120, 1469-7645},
