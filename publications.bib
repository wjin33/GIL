	pages = {660--665},
	file = {Cochard et al. - 2024 - Propagation of extended fractures by local nucleat.pdf:/Users/wencheng/Zotero/storage/QZZDSCEZ/Cochard et al. - 2024 - Propagation of extended fractures by local nucleat.pdf:application/pdf},
}

