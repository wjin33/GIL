}

@article{isaenkov_automated_2021,
	title = {An automated system for continuous monitoring of {CO2} geosequestration using multi-well offset {VSP} with permanent seismic sources and receivers: {Stage} 3 of the {CO2CRC} {Otway} {Project}},
