	author = {Jirásek, Milan and Zimmermann, Thomas},
	year = {2001},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/1097-0207\%2820010228\%2950\%3A6\%3C1291\%3A\%3AAID-NME12\%3E3.0.CO\%3B2-Q},
