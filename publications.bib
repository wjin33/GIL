@article{zappone_fault_2021,
	title = {Fault sealing and caprock integrity for {CO}$_{\textrm{2}}$ storage: an in situ injection experiment},
	volume = {12},
	issn = {1869-9510},
	shorttitle = {Fault sealing and caprock integrity for {CO}$_{\textrm{2}}$ storage},
	url = {https://se.copernicus.org/articles/12/319/2021/},
	doi = {10.5194/se-12-319-2021},
	abstract = {{\textless}p{\textgreater}{\textless}strong class="journal-contentHeaderColor"{\textgreater}Abstract.{\textless}/strong{\textgreater} The success of geological carbon storage depends on the assurance of permanent containment for injected carbon dioxide (CO{\textless}span class="inline-formula"{\textgreater}$_{\textrm{2}}${\textless}/span{\textgreater}) in the storage formation at depth. One of the critical elements of the safekeeping of CO{\textless}span class="inline-formula"{\textgreater}$_{\textrm{2}}${\textless}/span{\textgreater} is the sealing capacity of the caprock overlying the storage formation despite faults and/or fractures, which may occur in it. In this work, we present an ongoing injection experiment performed in a fault hosted in clay at the Mont Terri underground rock laboratory (NW Switzerland). The experiment aims to improve our understanding of the main physical and chemical mechanisms controlling (i) the migration of CO{\textless}span class="inline-formula"{\textgreater}$_{\textrm{2}}${\textless}/span{\textgreater} through a fault damage zone, (ii) the interaction of the CO{\textless}span class="inline-formula"{\textgreater}$_{\textrm{2}}${\textless}/span{\textgreater} with the neighboring intact rock, and (iii) the impact of the injection on the transmissivity in the fault. To this end, we inject CO{\textless}span class="inline-formula"{\textgreater}$_{\textrm{2}}${\textless}/span{\textgreater}-saturated saline water in the top of a 3 m thick fault in the Opalinus Clay, a clay formation that is a good analog of common caprock for CO{\textless}span class="inline-formula"{\textgreater}$_{\textrm{2}}${\textless}/span{\textgreater} storage at depth. The mobility of the CO{\textless}span class="inline-formula"{\textgreater}$_{\textrm{2}}${\textless}/span{\textgreater} within the fault is studied at the decameter scale by using a comprehensive monitoring system. Our experiment aims to close the knowledge gap between laboratory and reservoir scales. Therefore, an important aspect of the experiment is the decameter scale and the prolonged duration of observations over many months. We collect observations and data from a wide range of monitoring systems, such as a seismic network, pressure temperature and electrical conductivity sensors, fiber optics, extensometers, and an in situ mass spectrometer for dissolved gas monitoring. The observations are complemented by laboratory data on collected fluids and rock samples. Here we show the details of the experimental concept and installed instrumentation, as well as the first results of the preliminary characterization. An analysis of borehole logging allows for identifying potential hydraulic transmissive structures within the fault zone. A preliminary analysis of the injection tests helped estimate the transmissivity of such structures within the fault zone and the pressure required to mechanically open such features. The preliminary tests did not record any induced microseismic events. Active seismic tomography enabled sharp imaging the fault zone.{\textless}/p{\textgreater}},
	language = {English},
	number = {2},
	urldate = {2022-04-25},
	journal = {Solid Earth},
	author = {Zappone, Alba and Rinaldi, Antonio Pio and Grab, Melchior and Wenning, Quinn C. and Roques, Clément and Madonna, Claudio and Obermann, Anne C. and Bernasconi, Stefano M. and Brennwald, Matthias S. and Kipfer, Rolf and Soom, Florian and Cook, Paul and Guglielmi, Yves and Nussbaum, Christophe and Giardini, Domenico and Mazzotti, Marco and Wiemer, Stefan},
