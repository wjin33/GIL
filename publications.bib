	keywords = {DuMu, microbial, field modeling},
	pages = {595--606},
	file = {Springer Full Text PDF:/Users/wencheng/Zotero/storage/XJDA3JVD/Hagemann et al. - 2016 - Hydrogenization of underground storage of natural .pdf:application/pdf},
}

