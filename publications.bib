	author = {Grady II, Thomas J. and Khan, Rishi and Louboutin, Mathias and Yin, Ziyi and Witte, Philipp A. and Chandra, Ranveer and Hewett, Russell J. and Herrmann, Felix J.},
	month = may,
	year = {2022},
	note = {arXiv:2204.01205 [cs, math]},
	keywords = {Neural Operator},
