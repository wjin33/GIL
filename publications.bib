	file = {Yang et al. - 2020 - A scalable parallel computing SPH framework for pr.pdf:/Users/wencheng/Zotero/storage/W5VZ93QV/Yang et al. - 2020 - A scalable parallel computing SPH framework for pr.pdf:application/pdf},
}

@article{minatti_sph_2015,
