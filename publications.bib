	file = {He et al. - 2022 - A Droplet-Manipulation Method Based on the Magneti.pdf:/Users/wencheng/Zotero/storage/GKJ4K8DX/He et al. - 2022 - A Droplet-Manipulation Method Based on the Magneti.pdf:application/pdf},
}

@article{wu_particlepore_2021,
	title = {Particle–pore scale modelling of particle–fluid flows},
