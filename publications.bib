	author = {Jin, Wencheng and Arson, Chloé},
	month = dec,
	year = {2019},
	keywords = {Micromechanics, Anisotropy, Cohesive zone model, Damage to fracture transition, Extended finite element method, Nonlocal damage model},
	pages = {112617},
	file = {Jin and Arson - 2019 - XFEM to couple nonlocal micromechanics damage with.pdf:/Users/wencheng/Zotero/storage/F5LEHDG6/Jin and Arson - 2019 - XFEM to couple nonlocal micromechanics damage with.pdf:application/pdf},
}

@article{lu_flow_2021-1,
	title = {Flow and {Arching} of {Biomass} {Particles} in {Wedge}-{Shaped} {Hoppers}},
