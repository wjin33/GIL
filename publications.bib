	note = {Number: 19
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {VISTA},
	pages = {6089},
	file = {Banks et al. - 2021 - Geochemical Changes Associated with High-Temperatu.pdf:/Users/wencheng/Zotero/storage/A3U25UU3/Banks et al. - 2021 - Geochemical Changes Associated with High-Temperatu.pdf:application/pdf},
