	urldate = {2022-02-07},
	journal = {Renewable and Sustainable Energy Reviews},
	author = {Thaysen, Eike M. and McMahon, Sean and Strobel, Gion J. and Butler, Ian B. and Ngwenya, Bryne T. and Heinemann, Niklas and Wilkinson, Mark and Hassanpouryouzband, Aliakbar and McDermott, Christopher I. and Edlmann, Katriona},
