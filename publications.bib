	journal = {Computer Physics Communications},
	author = {Osorno, M. and Schirwon, M. and Kijanski, N. and Sivanesapillai, R. and Steeb, H. and Göddeke, D.},
	month = oct,
	year = {2021},
	keywords = {Porous media, Smoothed particle hydrodynamics, GPU computing, High performance computing, Single-phase flow},
	pages = {108059},
