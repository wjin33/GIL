@article{ni_comparative_2020,
	title = {Comparative {Life}-{Cycle} {Assessment} of {Aquifer} {Thermal} {Energy} {Storage} {Integrated} with in {Situ} {Bioremediation} of {Chlorinated} {Volatile} {Organic} {Compounds}},
