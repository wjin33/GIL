	file = {Zhang et al. - 2020 - Fault Reactivation and Induced Seismicity During M.pdf:/Users/wencheng/Zotero/storage/V6EXZIJN/Zhang et al. - 2020 - Fault Reactivation and Induced Seismicity During M.pdf:application/pdf},
}

@article{dezhi_lattice-based_2022,
	title = {A {Lattice}-{Based} {Predictive} {Model} for {Interaction} {Mode} of {Hydraulic} {Fracture} with {Natural} {Fractures}},
