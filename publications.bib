	journal = {Scientific Reports},
	author = {Lin, Chuandong and Luo, Kai Hong and Fei, Linlin and Succi, Sauro},
	month = nov,
	year = {2017},
	note = {Number: 1
Publisher: Nature Publishing Group},
	keywords = {pore modeling},
