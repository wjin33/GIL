	file = {Jello et al. - 2022 - Advanced geothermal energy storage systems by repu.pdf:/Users/wencheng/Zotero/storage/9AIAXXMB/Jello et al. - 2022 - Advanced geothermal energy storage systems by repu.pdf:application/pdf},
}

