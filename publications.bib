	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/nag.3428},
	keywords = {Hypoplasticity},
	pages = {2817--2848},
	file = {Snapshot:/Users/wencheng/Zotero/storage/AYGBUFYK/nag.html:text/html;Tafili et al. - 2022 - Constitutive anamnesis model (CAM) for fine-graine.pdf:/Users/wencheng/Zotero/storage/JQGJ9N9L/Tafili et al. - 2022 - Constitutive anamnesis model (CAM) for fine-graine.pdf:application/pdf},
