	file = {Chen et al. - 2020 - GPU-accelerated smoothed particle hydrodynamics mo.pdf:/Users/wencheng/Zotero/storage/AW4V66CG/Chen et al. - 2020 - GPU-accelerated smoothed particle hydrodynamics mo.pdf:application/pdf},
}

