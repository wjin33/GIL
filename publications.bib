	keywords = {porous media, multiphase flow, micromodels, complex fluids, microfluidics},
	pages = {1703575},
	file = {Anbari et al. - 2018 - Microfluidic Model Porous Media Fabrication and A.pdf:/Users/wencheng/Zotero/storage/ZZHZELD4/Anbari et al. - 2018 - Microfluidic Model Porous Media Fabrication and A.pdf:application/pdf;Snapshot:/Users/wencheng/Zotero/storage/8XGMEU97/smll.html:text/html},
