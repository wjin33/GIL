@article{chen_numerical_2022,
	title = {Numerical {Simulation} of {Proppant} {Transport} {Coupled} with {Multi}-{Planar}-{3D} {Hydraulic} {Fracture} {Propagation} for {Multi}-{Cluster} {Fracturing}},
