	file = {Soleimani and Weißenfels - 2021 - Numerical simulation of pile installations in a hy.pdf:/Users/wencheng/Zotero/storage/IITZCEHS/Soleimani and Weißenfels - 2021 - Numerical simulation of pile installations in a hy.pdf:application/pdf},
}

@article{nikooei_investigating_2021,
	title = {Investigating the effect of mixing layer rheology on granular flow over entrainable beds using {SPH} method},
