	pages = {457},
	file = {Lewicki et al. - 2006 - Natural and industrial analogues for leakage of CO.pdf:/Users/wencheng/Zotero/storage/GBQTFWEF/Lewicki et al. - 2006 - Natural and industrial analogues for leakage of CO.pdf:application/pdf},
}

@article{vilarrasa_potential_2015,
