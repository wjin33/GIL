@article{liu_analysis_2023,
	title = {Analysis of large-scale performance with supercritical-water fracturing in geothermal resource development},
	volume = {223},
	issn = {2949-8910},
	url = {https://www.sciencedirect.com/science/article/pii/S2949891023001628},
	doi = {10.1016/j.geoen.2023.211575},
	abstract = {Superhot geothermal resources are widely distributed in the Earth's crust. However, due to the extremely low permeability of the deep reservoirs, mining the heat is challenging. It has been known that supercritical water fracturing can generate complex cloud-fracture networks in granite rocks. These complex cloud-fracture networks greatly improve the permeability of the low permeable rocks and are therefore desirable for superhot geothermal developments. To date, the performance of fracturing with supercritical water has been only verified in laboratory experiments and simulations at a laboratory scale. It is still unknown whether supercritical water fracturing can be used to induce such complex cloud-fracture networks in large-scale developments. In this study, we developed a sequential multiscale model to reproduce the fracturing behaviors induced by supercritical water injection by coupling a microscale model and a macroscale model. The microscale model simulates processes of fracture initiation and propagation on fine-scale subgrids. The macroscale model reflects the results from the microscale model and simulates the pressure and stress fields on a large scale. The results suggest that supercritical water fracturing can generate complex cloud-fracture networks even on a large scale under certain conditions. The performances of supercritical water fracturing are sensitive to geological conditions of the rock masses and injection conditions. Our simulation results show that SC-water fracturing has the potential to enhance permeability and provide sufficient heat exchange area in superhot geothermal resources.},
	urldate = {2025-01-20},
	journal = {Geoenergy Science and Engineering},
	author = {Liu, Bailong and Suzuki, Anna and Yang, Deyi and Ito, Takatoshi},
	month = apr,
	year = {2023},
	keywords = {Numerical modeling, Japan group},
	pages = {211575},
	file = {Liu et al. - 2023 - Analysis of large-scale performance with supercrit.pdf:/Users/wencheng/Zotero/storage/35TGP84N/Liu et al. - 2023 - Analysis of large-scale performance with supercrit.pdf:application/pdf;ScienceDirect Snapshot:/Users/wencheng/Zotero/storage/L584YAAH/S2949891023001628.html:text/html},
}

@article{jiwei_numerical_2024,
	title = {Numerical simulation of fracture propagation morphology in hydraulic fracturing development of geothermal reservoirs based on the {CDEM}-{THM3D}},
	volume = {172},
	issn = {0266-352X},
	url = {https://www.sciencedirect.com/science/article/pii/S0266352X2400380X},
	doi = {10.1016/j.compgeo.2024.106444},
	abstract = {Reservoir fracturing stimulation is the key to constructing an enhanced geothermal system (EGS) for geothermal development in hot dry rock (HDR) reservoir. To clarify the crack propagation law of HDR fracturing, a 3D thermo-hydro-mechanical coupling simulation model of fracture propagation is produced based on the continuum–discontinuum element method (CDEM-THM3D). The correctness of the CDEM-THM3D model is validated by the theoretical solution of the nonisothermal soil consolidation model and Penny fracture model. Then, hydraulic fracturing numerical simulations are performed to analyse the influence of controlling variables on fracture propagation. The results indicate that the thermal tensile stress induced by injecting cold water can decrease reservoir fracture pressure and fracture extension pressure, causing an increasement in fracture width and a reduction in fracture length. Increasing thermal expansion coefficient and temperature difference enhances the effect of thermal stresses and even creates new branch fractures. A large elastic modulus favours an increase in fracture length, while large rock tensile strength and minimum horizontal stress lead to a decrease in fracture length. With increasing injection flow rate and fracturing fluid viscosity, the reservoir fracture pressure and the fracture width rise significantly, and the fracture easily breaks through the barrier of the high-stress compartment.},
	urldate = {2025-01-20},
	journal = {Computers and Geotechnics},
