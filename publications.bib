	keywords = {G2C},
	pages = {geoenergy2023},
	file = {Snapshot:/Users/wencheng/Zotero/storage/LJVBUZ3I/geoenergy2023-029.html:text/html;Todd et al. - 2024 - Modelling physical controls on mine water heat sto.pdf:/Users/wencheng/Zotero/storage/R62PANFS/Todd et al. - 2024 - Modelling physical controls on mine water heat sto.pdf:application/pdf},
}

@inproceedings{tancik_fourier_2020,
