@article{ellis_model_2024,
	title = {Model predictions of global geologic hydrogen resources},
	volume = {10},
	url = {https://www.science.org/doi/10.1126/sciadv.ado0955},
	doi = {10.1126/sciadv.ado0955},
	abstract = {Geologic hydrogen could be a low-carbon primary energy resource; however, the magnitude of Earth’s subsurface endowment has not yet been assessed. Knowledge of the occurrence and behavior of natural hydrogen on Earth has been combined with information from geologic analogs to construct a mass balance model to predict the resource potential. Given the associated uncertainty, stochastic model results predict a wide range of values for the potential in-place hydrogen resource [103 to 1010 million metric tons (Mt)] with the most probable value of {\textasciitilde}5.6 × 106 Mt. Although most of this hydrogen is likely to be impractical to recover, a small fraction (e.g., 1 × 105 Mt) would supply the projected hydrogen needed to reach net-zero carbon emissions for {\textasciitilde}200 years. This amount of hydrogen contains more energy ({\textasciitilde}1.4 × 1016 MJ) than all proven natural gas reserves on Earth ({\textasciitilde}8.4 × 1015 MJ). Study results demonstrate that further research into understanding the potential for geologic hydrogen resources is merited.},
	number = {50},
	urldate = {2024-12-14},
	journal = {Science Advances},
	author = {Ellis, Geoffrey S. and Gelman, Sarah E.},
	month = dec,
	year = {2024},
