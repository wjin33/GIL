	title = {Machine learning for hydrothermal treatment of biomass: {A} review},
	volume = {370},
	issn = {0960-8524},
	shorttitle = {Machine learning for hydrothermal treatment of biomass},
	url = {https://www.sciencedirect.com/science/article/pii/S0960852422018806},
	doi = {10.1016/j.biortech.2022.128547},
	abstract = {Hydrothermal treatment (HTT) (i.e., hydrothermal carbonization, liquefaction, and gasification) is a promising technology for biomass valorization. However, diverse variables, including biomass compositions and hydrothermal processes parameters, have impeded in-depth mechanistic understanding on the reaction and engineering in HTT. Recently, machine learning (ML) has been widely employed to predict and optimize the production of biofuels, chemicals, and materials from HTT by feeding experimental data. This review comprehensively analyzed the application of ML for HTT of biomass and systematically illustrated basic ML procedure and descriptors for inputs and outputs of ML models (e.g., biomass compositions, operation conditions, yield and physicochemical properties of derived products) that could be applied in HTT. Moreover, this review summarized ML-aided HTT prediction of yield, compositions, and physicochemical properties of HTT hydrochar or biochar, bio-oil, syngas, and aqueous phase. Ultimately, future prospects were proposed to enhance predictive performance, mechanistic interpretation, process optimization, data sharing, and model application during ML-aided HTT.},
	urldate = {2024-07-16},
	journal = {Bioresource Technology},
	author = {Zhang, Weijin and Chen, Qingyue and Chen, Jiefeng and Xu, Donghai and Zhan, Hao and Peng, Haoyi and Pan, Jian and Vlaskin, Mikhail and Leng, Lijian and Li, Hailong},
	month = feb,
	year = {2023},
	keywords = {Machine learning, Hydrothermal},
	pages = {128547},
	file = {ScienceDirect Snapshot:/Users/wencheng/Zotero/storage/LJJIV4ST/S0960852422018806.html:text/html;Zhang et al. - 2023 - Machine learning for hydrothermal treatment of bio.pdf:/Users/wencheng/Zotero/storage/4S6DBF2S/Zhang et al. - 2023 - Machine learning for hydrothermal treatment of bio.pdf:application/pdf},
}

