	file = {Daley et al. - 2016 - Field testing of modular borehole monitoring with .pdf:/Users/wencheng/Zotero/storage/KC4K8KIH/Daley et al. - 2016 - Field testing of modular borehole monitoring with .pdf:application/pdf},
}

