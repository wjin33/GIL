	keywords = {triggering mechanism},
	pages = {eaav7172},
	file = {Full Text PDF:/Users/wencheng/Zotero/storage/85NP4NIE/Eyre et al. - 2019 - The role of aseismic slip in hydraulic fracturing–.pdf:application/pdf},
}

@article{schultz_risk-based_2021,
	title = {A risk-based approach for managing hydraulic fracturing–induced seismicity},
	volume = {372},
	url = {https://www.science.org/doi/full/10.1126/science.abg5451},
	doi = {10.1126/science.abg5451},
	abstract = {Risks from induced earthquakes are a growing concern that needs effective management. For hydraulic fracturing of the Eagle Ford shale in southern Texas, we developed a risk-informed strategy for choosing red-light thresholds that require immediate well shut-in. We used a combination of datasets to simulate spatially heterogeneous nuisance and damage impacts. Simulated impacts are greater in the northeast of the play and smaller in the southwest. This heterogeneity is driven by concentrations of population density. Spatially varying red-light thresholds normalized on these impacts [moment magnitude (Mw) 2.0 to 5.0] are fairer and safer than a single threshold applied over a broad area. Sensitivity tests indicate that the forecast maximum magnitude is the most influential parameter. Our method provides a guideline for traffic light protocols and managing induced seismicity risks.},
	number = {6541},
