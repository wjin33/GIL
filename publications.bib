	url = {https://www.sciencedirect.com/science/article/pii/S0892687523003321},
	doi = {10.1016/j.mineng.2023.108318},
	abstract = {Rock fracturing through microwave irradiation has received significant attention recently as a viable pretreatment for improving the energy efficiency of comminution processes. This study presents a numerical analysis on the effects of microwave heating on the mechanical properties of hard rock. In particular, the reduction of the uniaxial compressive and tensile strength of granite-like rock due to microwave irradiation induced damage is numerically assessed. Rock fracture is modelled by a damage-viscoplasticity model, with separate damage variables for tension and compression types of failure. A global solution strategy is developed where first the electromagnetic problem is solved in COMSOL multiphysics software, then its solution is used as an input for the thermo-mechanical problem, which is finally solved by means of a staggered explicit solution method. Due to the preeminence of the thermal radiation, the thermal and the mechanical parts of the problem are considered as uncoupled. The model behaviour is tested in 3D finite element simulations of three-mineral numerical rock specimens, with mesostructures explicitly defined, pretreated first in a microwave oven and then subjected to uniaxial compression and tension tests. The results show that the compressive and tensile strength of rock can be considerably reduced by the microwave irradiation pretreatment.},
	urldate = {2024-12-05},
	journal = {Minerals Engineering},
	author = {Pressacco, Martina and Kangas, Jari J. J. and Saksala, Timo},
	month = nov,
	year = {2023},
	keywords = {COMSOL, Numerical modeling, FEM},
	pages = {108318},
	file = {Pressacco et al. - 2023 - Numerical modelling of microwave irradiated rock f.pdf:/Users/wencheng/Zotero/storage/EVRFCAC3/Pressacco et al. - 2023 - Numerical modelling of microwave irradiated rock f.pdf:application/pdf;ScienceDirect Snapshot:/Users/wencheng/Zotero/storage/7UYVQBVN/S0892687523003321.html:text/html},
}

@article{pressacco_numerical_2022,
	title = {Numerical {Modelling} of {Microwave} {Heating} {Assisted} {Rock} {Fracture}},
