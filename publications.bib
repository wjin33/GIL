	file = {ScienceDirect Snapshot:/Users/wencheng/Zotero/storage/46FNF4FU/S0001868624000666.html:text/html;Sharma et al. - 2024 - Progress in microwave absorbing materials A criti.pdf:/Users/wencheng/Zotero/storage/CCBST4XY/Sharma et al. - 2024 - Progress in microwave absorbing materials A criti.pdf:application/pdf},
}

@misc{teimoori_twenty_2021,
