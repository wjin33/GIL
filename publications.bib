	pages = {1--52},
	file = {Jiang et al. - 2016 - The material point method for simulating continuum.pdf:/Users/wencheng/Zotero/storage/42EPTU76/Jiang et al. - 2016 - The material point method for simulating continuum.pdf:application/pdf},
