	pages = {53--60},
	file = {Yin et al. - 2011 - Coupled THMC modeling of CO2 injection by finite e.pdf:/Users/wencheng/Zotero/storage/3SDWWN97/Yin et al. - 2011 - Coupled THMC modeling of CO2 injection by finite e.pdf:application/pdf},
}

@article{fu_thermal_2016,
