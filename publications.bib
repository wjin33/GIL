	pages = {113--144},
	file = {Jin and Arson - 2020 - Fluid-driven transition from damage to fracture in.pdf:/Users/wencheng/Zotero/storage/77H4J9AS/Jin and Arson - 2020 - Fluid-driven transition from damage to fracture in.pdf:application/pdf},
}

