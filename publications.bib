	pages = {1070--1078},
	file = {Full Text PDF:/Users/wencheng/Zotero/storage/TKDVE2HE/Peng et al. - 2012 - Numerical Analysis of Heat Transfer Characteristic.pdf:application/pdf},
}

@article{wang_thermal_2014,
	title = {Thermal stress {FEM} analysis of rock with microwave energy},
