	author = {Faust, Erik and Schlüter, Alexander and Müller, Henning and Steinmetz, Felix and Müller, Ralf},
	month = feb,
	year = {2024},
	keywords = {Lattice Boltzmann method, Elastodynamics, boundary conditions, Transient solid simulation},
	pages = {317--339},
	file = {Faust et al. - 2024 - Dirichlet and Neumann boundary conditions in a lat.pdf:/Users/wencheng/Zotero/storage/EF2HI42V/Faust et al. - 2024 - Dirichlet and Neumann boundary conditions in a lat.pdf:application/pdf},
