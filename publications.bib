	note = {Publisher: Cambridge University Press},
	keywords = {{\textbackslash}mu(I)},
	pages = {378--408},
	file = {Lagrée et al. - 2011 - The granular column collapse as a continuum valid.pdf:/Users/wencheng/Zotero/storage/S5NBNDTW/Lagrée et al. - 2011 - The granular column collapse as a continuum valid.pdf:application/pdf;Snapshot:/Users/wencheng/Zotero/storage/25XU3Y6K/D19642DE42D508E909AE9C05D55CCFC1.html:text/html},
}

