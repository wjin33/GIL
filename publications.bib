	keywords = {Mechanical behavior, Fundamentals},
	pages = {7705},
	file = {Full Text PDF:/Users/wencheng/Zotero/storage/6ZWG8AG4/Violay et al. - 2017 - Porosity evolution at the brittle-ductile transiti.pdf:application/pdf},
}

