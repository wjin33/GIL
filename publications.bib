@article{tafili_constitutive_2022,
	title = {Constitutive anamnesis model ({CAM}) for fine-grained soils},
	volume = {46},
	copyright = {© 2022 The Authors. International Journal for Numerical and Analytical Methods in Geomechanics published by John Wiley \& Sons Ltd.},
