@article{winterleitner_impact_2018,
	title = {The {Impact} of {Reservoir} {Heterogeneities} on {High}-{Temperature} {Aquifer} {Thermal} {Energy} {Storage} {Systems}. {A} {Case} {Study} from {Northern} {Oman}.},
	volume = {74},
