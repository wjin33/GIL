	pages = {8757--8773},
	file = {Shi et al. - 2020 - Impacts of the subsurface storage of natural gas a.pdf:/Users/wencheng/Zotero/storage/C3LQGME7/Shi et al. - 2020 - Impacts of the subsurface storage of natural gas a.pdf:application/pdf},
}

