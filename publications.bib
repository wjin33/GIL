	issn = {0959-6526},
	url = {https://www.sciencedirect.com/science/article/pii/S095965261832657X},
	doi = {10.1016/j.jclepro.2018.08.296},
	abstract = {Because over 60\% of the total energy consumed in conventional mining consists of excavating, hauling, and liberating processes, the energy consumption in conventional methods of mining has increased exponentially with the decline in global ore-grades. In-situ leaching (ISL) is an alternative mining technology, which eliminates these processes. However, ISL is currently limited to porous rock. Expansion of ISL to impervious mineral bearing rocks requires controlled, artificial fracture stimulation and the current practice of hydraulic fracturing is not compatible due to the associated environmental impacts and uncontrolled fracture propagation. Therefore, to extend the application of ISL to impervious rocks, an alternative fracturing method using Soundless Cracking Demolition Agents (SCDAs), which is capable of producing controlled fractures in a rock mass, is proposed in this study. With the aim of understanding the fracturing performance of SCDA under in-situ stress conditions prevalent in ISL environments, a 3D numerical model for SCDA charging was produced using Particle Flow Code (PFC3D 5.0) and validated with experimentally obtained data. The model was then extended to investigate the fracture mechanism of SCDA charging at various confining pressures of 5, 10, 15, and 20 MPa. Simulation results suggest that during SCDA charging, the number of radial fracture propagations and the total fracture damage in the rock mass increase with the confinement and the fracture pattern is dependent on the confining stress of the surrounding rock. In comparison to a hydraulically fractured specimen with identical dimensions, the fracture density of SCDA charged specimen showed a ten-fold increase at high confining pressures (20 MPa). Furthermore, as opposed to an uncontrolled, unidirectional diametrical fracture generated by hydraulic fracturing, SCDA charging produces multiple radial fractures around the injection well facilitating better spatial distribution of fluid flow for ISL. The results of this study imply that SCDA charging produces a denser and a controllable fracture network compared to hydraulic fracturing, which could potentially expand ISL applications to impervious rocks.},
	language = {en},
	urldate = {2022-03-23},
	journal = {Journal of Cleaner Production},
	author = {De Silva, V. R. S. and Ranjith, P. G. and Perera, M. S. A. and Wu, B. and Wanniarachchi, W. A. M.},
	month = dec,
	year = {2018},
	keywords = {ISL, SCDA},
	pages = {586--606},
	file = {De Silva et al. - 2018 - A low energy rock fragmentation technique for in-s.pdf:/Users/wencheng/Zotero/storage/UD2TSDMA/De Silva et al. - 2018 - A low energy rock fragmentation technique for in-s.pdf:application/pdf},
}

@article{zhao_mineral_2018,
	title = {Mineral alteration and pore-plugging caused by acid in situ leaching: a case study of the {Wuyier} uranium deposit, {Xinjiang}, {NW} {China}},
	volume = {11},
	issn = {1866-7538},
	shorttitle = {Mineral alteration and pore-plugging caused by acid in situ leaching},
	url = {https://doi.org/10.1007/s12517-018-4064-7},
	doi = {10.1007/s12517-018-4064-7},
	abstract = {Mineral alteration is the key factor affecting the extraction of uranium and pore-plugging during acid in situ leaching (ISL) of uranium deposit. A detailed study of mineral alteration in ISL will help us to better understand pore-plugging mechanism and enlighten us to develop the corresponding de-plugging method. The Wuyier uranium deposit in Yili Basin, Xinjiang Province, NW China, has been acid in situ leached (ISL) for more than 20 years; thus, it provides an excellent example for the study of mineralogical alteration during acid ISL. In this study, X-ray diffraction (XRD) and scanning electron microscope (SEM) have been used to study sandstone samples collected from ore-bearing aquifer of Wuyier Deposit. Results show that during acid ISL, quartz is less influenced while feldspar could be altered to kaolinite and quartz. According to the occurrence of minerals in sandstone pores, two types of sandstone pore-plugging are observed by SEM. The physical pore plugging is caused by accumulation of micro-authigenic quartz and clay particles derived from alteration of feldspar, while the precipitations of CaSO4, Al(OH)3, Fe(OH)3, and SiO2(aq) filling the pores of sandstones or covering the surface of rock grains lead to the chemical pore-plugging of ores.},
	language = {en},
	number = {22},
	urldate = {2022-03-23},
	journal = {Arabian Journal of Geosciences},
