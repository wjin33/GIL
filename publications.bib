@article{khoei_enriched_2015,
	title = {An enriched {FEM} technique for modeling hydraulically driven cohesive fracture propagation in impermeable media with frictional natural faults: {Numerical} and experimental investigations: {X}-{FEM} {MODELING} {OF} {HYDRAULIC} {FRACTURE} {PROPAGATION} {WITH} {NATURAL} {FAULTS}},
