	month = jan,
	year = {2020},
	doi = {10.1016/bs.aams.2019.11.001},
	keywords = {Large deformation, Contact, Fluid mechanics, Fracture, FSI, Solid mechanics},
	pages = {185--398},
	file = {de Vaucorbeil et al. - 2020 - Chapter Two - Material point method after 25 years.pdf:/Users/wencheng/Zotero/storage/MELMEZXH/de Vaucorbeil et al. - 2020 - Chapter Two - Material point method after 25 years.pdf:application/pdf;ScienceDirect Snapshot:/Users/wencheng/Zotero/storage/YUMCI6ZF/S0065215619300146.html:text/html},
}

@article{gao_gpu_2018,
