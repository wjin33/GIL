	month = jun,
	year = {2025},
	keywords = {Numerical Simulation, Computational Fluid Dynamics, Computational Solid Mechanics, Computer Modelling, Fluid–solid interaction, Molecular Deformation Dynamics, Solid Mechanics, The parallelizable local updating model, The robust framework},
	pages = {6041--6054},
}

