}

@article{li_fully_2017,
	title = {A fully coupled electromagnetic-thermal-mechanical model for coalbed methane extraction with microwave heating},
	volume = {46},
	issn = {1875-5100},
	url = {https://www.sciencedirect.com/science/article/pii/S1875510017303554},
	doi = {10.1016/j.jngse.2017.08.031},
	abstract = {Realized by the conversion of the electromagnetic energy into thermal energy, microwave heating can be a sound method to enhance coalbed methane (CBM) recovery. Although the effects of microwave heating on the petrophysical characteristics of coal have been comprehensively investigated, few studies considered the complex coal-gas interactions under non-isothermal conditions. In this study, a fully coupled electromagnetic-thermal-mechanical model was developed to investigate the influence of coal compaction, thermal expansion, thermal-induced gas desorption, and sorption-induced coal deformation on CBM extraction. The results indicated that the sorption-induced coal deformation makes the predominant contribution to the porosity evolution. In addition, microwave heating can enhance the cumulative gas production by 43.9\% by promoting gas desorption and seepage. The sensitivity analysis showed that the microwave operating at 2450 MHz under lower powers is preferred to maintain efficient stimulation and to avoid overheating. Furthermore, the stimulation efficiency of microwave heating increases with the initial permeability and gas pressure. Outcomes of this study can provide valuable insights into microwave-assisted CBM recovery.},
	urldate = {2024-12-03},
	journal = {Journal of Natural Gas Science and Engineering},
	author = {Li, He and Lin, Baiquan and Yang, Wei and Hong, Yidu and Wang, Zheng},
	month = oct,
	year = {2017},
	keywords = {COMSOL, Numerical modeling},
	pages = {830--844},
	file = {Li et al. - 2017 - A fully coupled electromagnetic-thermal-mechanical.pdf:/Users/wencheng/Zotero/storage/MHS4GENW/Li et al. - 2017 - A fully coupled electromagnetic-thermal-mechanical.pdf:application/pdf;ScienceDirect Snapshot:/Users/wencheng/Zotero/storage/X2CPGQWP/S1875510017303554.html:text/html},
}

@article{huang_coupled_2018,
	title = {A coupled electromagnetic irradiation, heat and mass transfer model for microwave heating and its numerical simulation on coal},
