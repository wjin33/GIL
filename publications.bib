}

@article{tao_coupled_2019,
	title = {Coupled {Thermo}-{Hydro}-{Mechanical}-{Chemical} {Modeling} of {Permeability} {Evolution} in a {CO} $_{\textrm{2}}$ -{Circulated} {Geothermal} {Reservoir}},
