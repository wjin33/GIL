@techreport{liu_iipg-based_2021,
	title = {An {IIPG}-{Based} {Finite} {Element} {Framework} in {MOOSE} for {Modeling} {Fiber} {Reinforced} {Composite} {Failure} {Governed} by {Extrinsic} {Cohesive} {Laws}},
