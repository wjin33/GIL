	note = {Publisher: American Chemical Society},
	keywords = {Torrefaction},
	pages = {3512--3531},
	file = {ACS Full Text Snapshot:/Users/wencheng/Zotero/storage/4HSBGTCM/acssuschemeng.html:text/html;Ciesielski et al. - 2020 - Advances in Multiscale Modeling of Lignocellulosic.pdf:/Users/wencheng/Zotero/storage/LT2UP5XQ/Ciesielski et al. - 2020 - Advances in Multiscale Modeling of Lignocellulosic.pdf:application/pdf},
